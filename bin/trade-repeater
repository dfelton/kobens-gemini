fi

ps aux | grep -v 'grep\|fill-time' | grep -q gemini\ repeater:archiver
if [ $? -eq 1 ]; then
    php -n $(dirname $BASH_SOURCE)/gemini repeater:archiver &
fi

ps aux | grep -v grep | grep -q gemini\ repeater:archiver:fill-time
if [ $? -eq 1 ]; then
    php -n $(dirname $BASH_SOURCE)/gemini repeater:archiver:fill-time &
fi

ps aux | grep -v grep | grep -q gemini\ repeater:fill-monitor-rest
if [ $? -eq 1 ]; then
    php -n $(dirname $BASH_SOURCE)/gemini repeater:fill-monitor-rest &
fi


#
# Statistics
#
ps aux | grep -v grep | grep -q gemini\ repeater:stats:daily-profit
if [ $? -eq 1 ]; then
    php -n $(dirname $BASH_SOURCE)/gemini repeater:stats:daily-profit &
