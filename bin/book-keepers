#!/bin/bash

#
# Book Keepers
#
ps aux | grep -v grep | grep -q 'gemini market:book-keeper aaveusd'
if [ $? -eq 1 ]; then
    echo starting book keeper aaveusd
    php -n $(dirname $BASH_SOURCE)/gemini market:book-keeper aaveusd &
    sleep 0.25
fi

