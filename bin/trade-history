#!/bin/bash

#
# Trade History Processes
#
ps aux | grep -v grep | grep -q 'gemini logger:trade-history aaveusd'
if [ $? -eq 1 ]; then
    echo starting trade history logger aaveusd
    php -n $(dirname $BASH_SOURCE)/gemini logger:trade-history aaveusd &
fi

