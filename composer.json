{
    "name": "kobens/kobens-gemini",
    "version": "0.0.1",
    "description": "For facilitating trades on the Gemini exchange.",
    "type": "project",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Darren Felton",
            "email": "darren_felton@hotmail.com"
        }
